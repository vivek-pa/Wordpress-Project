        <executionStrategy>once-per-session</executionStrategy>
        <threadSafe>false</threadSafe>
        <configuration>
            <parameters>
                <parameter>
                    <name>
                        <value lang="en">Build Number</value>
                    </name>
                    <type>Hidden</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>buildNumber</key>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>false</show>
                    <value>1</value>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">Environment</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.impl.EnvironmentsParameterImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.commons</groupId>
                                <artifactId>phresco-commons</artifactId>
                                <type>jar</type>
                                <version>2.0.0.27002</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>true</required>
                    <editable>true</editable>
                    <key>environmentName</key>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value></value>
                     <dependency>dataBase,fetchSql</dependency>
                </parameter>
                <parameter>
                    <pluginParameter>framework</pluginParameter>
                    <mavenCommands>
                        <mavenCommand>
                            <key>showErrors</key>
                            <value>-e</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>hideLogs</key>
                            <value>-q</value>
                        </mavenCommand>
                        <mavenCommand>
                            <key>showDebug</key>
                            <value>-X</value>
                        </mavenCommand>
                    </mavenCommands>
                    <name>
                        <value lang="en">Logs</value>
                    </name>
                    <type>List</type>
                    <required>false</required>
                    <editable>true</editable>
                    <key>logs</key>
                    <possibleValues>
                        <value key="showErrors">Show Errors</value>
                        <value key="hideLogs">Hide Logs</value>
                        <value key="showDebug">Show Debug</value>
                    </possibleValues>
                    <multiple>false</multiple>
                    <sort>false</sort>
                    <show>true</show>
                    <value></value>
                </parameter>
                 <parameter>
                    <name>
                        <value lang="en">Execute Sql</value>
                    </name>
                    <type>Boolean</type>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>executeSql</key>
                    <value>true</value>
                    <sort>false</sort>
                    <show>true</show>
                    <dependency>dataBase</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en">DataBase</value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.DynamicDataBaseImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.framework</groupId>
                                <artifactId>phresco-framework-impl</artifactId>
                                <type>jar</type>
                                <version>2.0.0.27002</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>dataBase</key>
                    <multiple>false</multiple>
                    <value>mysql</value>
                    <sort>false</sort>
                    <show>false</show>
                    <dependency>fetchSql</dependency>
                </parameter>
                <parameter>
                    <name>
                        <value lang="en"></value>
                    </name>
                    <type>DynamicParameter</type>
                    <dynamicParameter>
                        <class>com.photon.phresco.framework.param.impl.DynamicFetchSqlImpl</class>
                        <dependencies>
                            <dependency>
                                <groupId>com.photon.phresco.framework</groupId>
                                <artifactId>phresco-framework-impl</artifactId>
                                <type>jar</type>
                                <version>2.0.0.27002</version>
                            </dependency>
                        </dependencies>
                    </dynamicParameter>
                    <required>false</required>
                    <editable>true</editable>
                    <description></description>
                    <key>fetchSql</key>
                    <multiple>false</multiple>
                    <value></value>
                    <sort>true</sort>
                    <show>false</show>
                </parameter>                
            </parameters>
        </configuration>
    </mojo>
